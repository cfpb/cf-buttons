{
  "name": "fj-buttons",
  "private": true,
  "version": "0.5.3",
  "main": "src/buttons.less",
  "dependencies": {
    "fj-colors": "git://fake.ghe.domain/flapjack/fj-colors.git",
    "fj-typography": "git://fake.ghe.domain/flapjack/fj-typography.git",
    "font-awesome": "~3.2.1"
  },
  "exportsOverride": {
    "fj-*": {
      "css": "src/*.css",
      "less": "src/*.less",
      "fonts": "src/fonts/*.*"
    },
    "cfpb-font-icons": {
      "css": "src/*.css",
      "fonts": "static/fonts/*.*"
    },
    "font-awesome": {
      "css": "css/*.css",
      "fonts": "font/*.*"
    }
  }
}
